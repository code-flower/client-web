{
  "manifest_version": 2,
  "name": "CodeFlower-Test",
  "version": "0.1",
  "description": "Visualize repos",
  "icons": { 
    "48": "images/icon48.png",
    "128": "images/icon48.png" 
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_title": "CodeFlower-Test",
    "default_icon": "images/icon48.png"
  }
}